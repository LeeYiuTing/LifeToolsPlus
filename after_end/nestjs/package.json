{
  "name": "nest",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start -b swc --watch",
    "start:debug": "nest start swc --debug --watch",
    "start:prod": "node dist/main",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "migrations:gen": "typeorm migration:generate -d ./dist/database.config.js",
    "migrations:run": "typeorm migration:run -d ./dist/database.config.js",
    "migrations:revert": "typeorm migration:revert -d ./dist/database.config.js",
    "orm:schema:drop": "typeorm query -d ./dist/database.config.js 'DROP schema public cascade; CREATE schema public;'",
    "orm:schema:reset": "npm run typeorm:schema:drop && npm run migrations:run"
  },
  "dependencies": {
    "@nestjs/bullmq": "^10.1.0",
    "@nestjs/common": "^10.3.4",
    "@nestjs/config": "^3.2.2",
    "@nestjs/core": "^10.3.4",
    "@nestjs/event-emitter": "^2.0.4",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/platform-socket.io": "^10.3.7",
    "@nestjs/schedule": "^4.0.1",
    "@nestjs/swagger": "^7.3.0",
    "@nestjs/typeorm": "^10.0.2",
    "@nestjs/websockets": "^10.3.7",
    "@socket.io/redis-adapter": "^8.3.0",
    "async-lock": "^1.4.1",
    "bcrypt": "^5.1.1",
    "body-parser": "^1.20.2",
    "bullmq": "^5.4.5",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "cookie": "^0.6.0",
    "cookie-parser": "^1.4.6",
    "dayjs": "^1.11.10",
    "ioredis": "^5.3.2",
    "joi": "^17.12.3",
    "lodash": "^4.17.21",
    "lodash-es": "^4.17.21",
    "mysql2": "^3.9.2",
    "openid-client": "^5.6.5",
    "reflect-metadata": "^0.2.0",
    "rxjs": "^7.8.1",
    "sanitize-filename": "^1.6.3",
    "socket.io": "^4.7.5",
    "swagger-ui-express": "^5.0.0",
    "typeorm": "^0.3.20",
    "ua-parser-js": "^1.0.37"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.3.2",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@swc/cli": "^0.3.10",
    "@swc/core": "^1.4.8",
    "@types/bcrypt": "^5.0.2",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.2",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^20.3.1",
    "@types/supertest": "^6.0.0",
    "@types/ua-parser-js": "^0.7.39",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.5.0",
    "prettier": "^3.0.0",
    "source-map-support": "^0.5.21",
    "sql-formatter": "^15.3.0",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
